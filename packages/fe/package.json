    "start-db": "ts-node --files src/db/startDb.ts",
    "test": "yarn ../.. exec vitest run -c packages/front/vitest.root.config.ts --coverage",
    "type": "yarn ../.. exec tsc -p packages/front/tsconfig.json --noEmit --skipLibCheck && node -e \"console.log('Fe type success')\""
