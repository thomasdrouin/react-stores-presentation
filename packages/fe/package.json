    "@mui/material": "5.11.0",
    "@mui/styles": "5.11.0",
    "axios": "1.2.2",
    "dayjs": "1.11.5",
    "immer": "9.0.17",
    "local-storage": "2.0.0",
    "mongodb": "4.13.0",
    "mongodb-memory-server": "8.11.0",
    "react": "18.2.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
